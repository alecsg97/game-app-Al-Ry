

<h2>Celeb List</h2>
<div class="celeb__container">
    {{#each celeb}}
        <div class="celeb__box">
            <h3>Name: {{this.name}}</h3>
            <h3>Occupation: {{this.occupation}}</h3>
            <h3>Catchphrase: {{this.catchphrase}}</h3>
            {{!-- <a href="/animals/details/{{this._id}}">See More Details Here</a> --}}
        </div>
    {{/each}}
</div>

<h1>BOOKS</h1>
 
{{!-- since we passed allTheBooksFromDB as `books`,
we can iterate over it here with #each--}}
 
{{#each books}}
  <div>
    <h3>{{title}}</h3>
    <p>By: {{author}}</p> {{!-- additional book property --}}
    <a href="/books/{{_id}}">See details</a> {{!-- link that points to the details page --}}
  </div>
{{/each}}